{
  "name": "realtime-kit/tenant-broadcast",
  "description": "Tenant-aware helpers for Laravel Reverb broadcasting",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Realtime Kit",
      "email": "abdistar16@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/broadcasting": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "RealtimeKit\\TenantBroadcast\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RealtimeKit\\TenantBroadcast\\TenantBroadcastServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
